;connect '/C=14'
connect '192.168.0.9:4030 /nossh /T=1'
setecho 1
fileopen fhandle 'test-report.txt' 0

; set UTC offset
cmd = ':SG+01.0#'
call setcmd

; set longitude
cmd = ":Sg-05:38:00#"
call setcmd

; set latitude
cmd = ':St45:16:00#'
call setcmd

; set elevation
cmd = ':Su0200#'
call setcmd

fileclose fhandle
disconnect
end

:setcmd
filewrite fhandle cmd
sendln cmd
wait '0' '1'
if result=0 then
    filewriteln fhandle ' timeout FAILED'
elseif result=1 then
    filewriteln fhandle ' 0 rejected FAILED'
else
    filewriteln fhandle ' 1 OK'
endif
flushrecv
return
