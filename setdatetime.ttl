connect '/C=13'
setecho 0
fileopen fhandle 'test-report.txt' 0

; set date
cmd = ':SC09/28/14#'
filewrite fhandle cmd
send cmd
wait '0' '1'
if result=0 then
    filewriteln fhandle ' timeout FAILED'
elseif result=1 then
    filewriteln fhandle ' 0 rejected FAILED'
else
    filewriteln fhandle ' 1 OK'
endif
flushrecv

; set local time
cmd = ':SL16:17:0#'
filewrite fhandle cmd
send cmd
wait '0' '1'
if result=0 then
    filewriteln fhandle ' timeout FAILED'
elseif result=1 then
    filewriteln fhandle ' 0 rejected FAILED'
else
    filewriteln fhandle ' 1 OK'
endif
flushrecv

fileclose fhandle
disconnect
