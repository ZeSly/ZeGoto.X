connect '/C=13'
setecho 0
fileopen fhandle 'test-report.txt' 0

; test the name
cmd = ':GVP#'
filewrite fhandle cmd
send cmd
waitregex '#'
if timeout then
    filewriteln fhandle ' timeout FAILED'
else
    filewrite fhandle ' '
    filewrite fhandle inputstr
    filewriteln fhandle ' OK'
endif
flushrecv

; sending RA step target
cmd = ':ZSr200000#'
filewrite fhandle cmd
send cmd
wait '0' '1'
if result=0 then
    filewriteln fhandle ' timeout FAILED'
elseif result=1 then
    filewriteln fhandle ' 0 rejected FAILED'
else
    filewriteln fhandle ' 1 OK'
endif
flushrecv

cmd = ':ZGr#'
filewrite fhandle cmd
send cmd
waitregex '#'
if result=0 then
    filewriteln fhandle ' timeout FAILED'
else
    strcompare inputstr '200000#' 
    if result != 0 then
        filewrite fhandle ' '
        filewrite fhandle inputstr
        filewriteln fhandle ' bad response FAILED'
    else
        filewrite fhandle ' '
        filewrite fhandle inputstr
        filewriteln fhandle ' OK'
    endif
endif
flushrecv

